        </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="#FF266A2E" Offset="0.012"/>
        <GradientStop Color="#FF3E9548" Offset="0.547"/>
        <GradientStop Color="#FF266A2E" Offset="1"/>
